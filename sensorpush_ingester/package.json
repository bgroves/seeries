  "dependencies": {
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "pg": "^8.4.1",
    "pino": "^6.7.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/baretest": "^2.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.11.8",
    "@types/pg": "^7.14.5",
    "@types/pino": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "baretest": "^2.0.0",
    "eslint": "^7.16.0",
    "eslint-plugin-node": "^11.1.0",
    "nock": "^13.0.5",
    "pino-pretty": "^4.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.65",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build": "tsc --project ./",
    "dev": "ts-node-dev --transpile-only -- src/index.ts",
